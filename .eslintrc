{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends":[
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "globals": {
        "$": "readonly"
    },
    "rules": {
        "quotes":[2, "single"],
        "@typescript-eslint/no-unused-vars": [2, {
            "varsIgnorePattern": "^_",
            "argsIgnorePattern": "^_"
        }],
        "semi": ["error", "never"]
    }
}